@import 'nib'

@-webkit-keyframes test {
  0%{
    -webkit-transform: rotate(180deg) translateY(50px) translateX(50px);
    -webkit-transform-origin: top left;
    opacity: 0;
    }
  85%{
    -webkit-transform: rotate(0deg) translateY(0px) translateX(0%);
    -webkit-transform-origin: bottom right;
    opacity: 1;
  }
  92%{
    -webkit-transform:translateY(15px) scale(1.03);
  }
}

.test {
  animation-name:test
}

@keyframes vertShake
   0%, 20%, 50%, 80%, 100%
     transform: translateY(0)
   40%
     transform: translateY(-30px)
   60%
     transform: translateY(-15px)

.vertShake {
    animation-name: vertShake
}

@keyframes horShake
   0%, 10%, 30%, 50%, 70%, 90%, 100%
     transform: translateX(0)
   20%, 60%
     transform: translateX(-10px)
   40%, 80%
     transform: translateX(15px)

.horShake {
    animation-name: horShake
    animation-duration: 0.6s !important
}

@keyframes horLeftShake
   0%, 20%, 50%, 80%, 100%
     transform: translateX(0)
   40%
     transform: translateX(-30px)
   60%
